<% present @familiar do |presenter| %>

<%= title @familiar.name %>
<%= presenter.image(:div) %>
<%= presenter.median %>
<%= presenter.new_sale(@sale) %>
<div id="container" style="width:560px; height:300px;"></div>
<%= presenter.sales %>

<% end %>

<script type="text/javascript" charset="utf-8">
  $(function(){
    new Highcharts.Chart({
      chart: {
        renderTo: "container",
        type: 'column'
      },
      title: {
        text: "Mandrake"
      },
      xAxis: {
        categories:<%= @hash.keys.sort %>
      },
      yAxis: {
        title: {
          text: "min"
        }
      },
      series: [{
        data: <%= @hash.keys.sort.map{|e| @hash[e]} %>,
      }],
    });
  });
</script>
